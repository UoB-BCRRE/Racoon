# baseURI: http://purl.org/rail/darwin
# imports: http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v1
# imports: http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v2
# imports: http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v2/
# imports: http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v3
# imports: http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v3/
# imports: http://www.thalesgroup.com/rtti/PushPort/Forecasts/v3
# imports: http://www.thalesgroup.com/rtti/PushPort/Schedules/v2
# imports: http://www.thalesgroup.com/rtti/PushPort/Schedules/v3

@prefix : <http://purl.org/rail/darwin/> .
@prefix ct: <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v1/> .
@prefix ct2: <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v2/> .
@prefix ct3: <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v3/> .
@prefix ct4: <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v4/> .
@prefix for: <http://www.thalesgroup.com/rtti/PushPort/Forecasts/v3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp16: <http://www.thalesgroup.com/rtti/PushPort/v16/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sch2: <http://www.thalesgroup.com/rtti/PushPort/Schedules/v2/> .
@prefix sch3: <http://www.thalesgroup.com/rtti/PushPort/Schedules/v3/> .
@prefix status: <http://thalesgroup.com/RTTI/PushPortStatus/root_1/> .
@prefix tor: <http://www.thalesgroup.com/rtti/PushPort/TrainOrder/v1/> .
@prefix v3: <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v3#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/rail/darwin>
  a owl:Ontology ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v1> ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v2> ;
  owl:imports ct2: ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/CommonTypes/v3> ;
  owl:imports ct3: ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/Forecasts/v3> ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/Schedules/v2> ;
  owl:imports <http://www.thalesgroup.com/rtti/PushPort/Schedules/v3> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
:Journey
  a owl:Class ;
  rdfs:label "Journey" ;
  rdfs:subClassOf owl:Thing ;
.
:simpleTTPlatNum
  a owl:DatatypeProperty ;
  rdfs:label "simple platform number" ;
.
status:ErrorCodeType
  a rdfs:Datatype ;
  rdfs:comment "Error Code Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 1 ;
          ]
          [
            xsd:maxLength 32 ;
          ]
        ) ;
    ] ;
.
status:ErrorMsgType
  a rdfs:Datatype ;
  rdfs:comment "Error Message Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 128 ;
          ]
        ) ;
    ] ;
.
status:PPConnect
  a owl:Class ;
  rdfs:comment "Signal end of the setup phase and switch to use the requested PP data schema" ;
.
status:PPReqVersion
  a owl:Class ;
  rdfs:comment "Request the schema versions required by the client" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty status:ttrefversion ;
      owl:someValuesFrom [
          a rdfs:Datatype ;
          owl:onDatatype xsd:string ;
          owl:withRestrictions (
              [
                xsd:minLength "1" ;
              ]
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty status:ttversion ;
      owl:someValuesFrom [
          a rdfs:Datatype ;
          owl:onDatatype xsd:string ;
          owl:withRestrictions (
              [
                xsd:minLength "1" ;
              ]
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty status:version ;
      owl:someValuesFrom [
          a rdfs:Datatype ;
          owl:onDatatype xsd:string ;
          owl:withRestrictions (
              [
                xsd:minLength "1" ;
              ]
            ) ;
        ] ;
    ] ;
.
status:PPStatus
  a owl:Class ;
  rdfs:comment "Setup phase status/heartbeat response" ;
  rdfs:subClassOf status:StatusType ;
.
status:StatusType
  a owl:Class ;
  rdfs:comment "Status Code Type" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty status:code ;
      owl:someValuesFrom status:ErrorCodeType ;
    ] ;
.
status:code
  a owl:DatatypeProperty ;
  rdfs:comment "Status Code Type" ;
  rdfs:domain status:StatusType ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:intersectionOf (
          status:ErrorCodeType
          status:ErrorMsgType
        ) ;
    ] ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
status:ttrefversion
  a owl:DatatypeProperty ;
  rdfs:comment "The namespace of the Push Port Timetable Reference data schema supported by the client." ;
  rdfs:domain status:PPReqVersion ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 1 ;
          ]
        ) ;
    ] ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
status:ttversion
  a owl:DatatypeProperty ;
  rdfs:comment "The namespace of the Push Port Timetable schema supported by the client." ;
  rdfs:domain status:PPReqVersion ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 1 ;
          ]
        ) ;
    ] ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
status:version
  a owl:DatatypeProperty ;
  rdfs:comment "The namespace of the Push Port data schema supported by the client." ;
  rdfs:domain status:PPReqVersion ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 1 ;
          ]
        ) ;
    ] ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct:ActivityType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 2 ;
          ]
          [
            xsd:maxLength 12 ;
          ]
          [
            xsd:pattern "([A-Z0-9\\- ][A-Z0-9\\- ]){1,6}" ;
          ]
        ) ;
    ] ;
.
ct:CIFTrainCategoryType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 0 ;
          ]
          [
            xsd:maxLength 2 ;
          ]
        ) ;
    ] ;
.
ct:CIFTrainStatusType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 1 ;
          ]
          [
            xsd:pattern "[BFPST12345]" ;
          ]
        ) ;
    ] ;
.
ct:CircularTimes
  a owl:Class ;
  rdfs:comment "A scheduled time used to distinguish a location on circular routes. Note that all scheduled time attributes are marked as optional, but at least one must always be supplied. Only one value is required, and typically this should be the wtd value. However, for locations that have no wtd, or for clients that deal exclusively with public times, another value that is valid for the location may be supplied." ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty ct:pta ;
      owl:someValuesFrom ct:WTimeType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty ct:ptd ;
      owl:someValuesFrom ct:WTimeType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty ct:wta ;
      owl:someValuesFrom ct:WTimeType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty ct:wtd ;
      owl:someValuesFrom ct:WTimeType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty ct:wtp ;
      owl:someValuesFrom ct:WTimeType ;
    ] ;
.
ct:CrsType
  a rdfs:Datatype ;
  rdfs:comment "CRS Code Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 3 ;
          ]
        ) ;
    ] ;
.
ct:DCISRequestID
  a rdfs:Datatype ;
  rdfs:comment "A DCIS client request identifier" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 1 ;
          ]
          [
            xsd:maxLength 16 ;
          ]
          [
            xsd:pattern "[-_A-Za-z0-9]{1,16}" ;
          ]
        ) ;
    ] ;
.
ct:DelayValueType
  a rdfs:Datatype ;
  owl:equivalentClass xsd:short ;
.
ct:DisruptionReasonType
  a owl:Class ;
  rdfs:comment "Type used to represent a cancellation or late running reason" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ct:ReasonCodeType ;
      owl:onProperty ct:hasReasonCodeType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ct:TiplocType ;
      owl:onProperty ct:tiploc ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty ct:near ;
    ] ;
.
ct:PlatformType
  a rdfs:Datatype ;
  rdfs:comment "Defines a platform number" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 1 ;
          ]
          [
            xsd:maxLength 3 ;
          ]
        ) ;
    ] ;
.
ct:RIDType
  a rdfs:Datatype ;
  rdfs:comment "RTTI Train ID Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 16 ;
          ]
        ) ;
    ] ;
.
ct:RTTIDateTimeType
  a rdfs:Datatype ;
  rdfs:comment "RTTI DateTime Type (basic XML date/time)" ;
  owl:equivalentClass xsd:dateTime ;
.
ct:RTTIDateType
  a rdfs:Datatype ;
  rdfs:comment "RTTI Date Type (basic XML date)" ;
  owl:equivalentClass xsd:dateTime ;
.
ct:RTTITimeType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:pattern "([0-1][0-9]|2[0-3]):[0-5][0-9]" ;
          ]
        ) ;
    ] ;
.
ct:ReasonCodeType
  a rdfs:Datatype ;
  rdfs:comment "A Darwin Reason Code" ;
  owl:equivalentClass xsd:short ;
.
ct:SnapshotIDType
  a rdfs:Datatype ;
  rdfs:comment "Defines the ID for a snapshot file to be recovered via FTP" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:maxLength 40 ;
          ]
        ) ;
    ] ;
.
ct:SourceTypeInst
  a rdfs:Datatype ;
  rdfs:comment "RTTI CIS code, provided if forecast or actual source type is CIS" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 4 ;
          ]
        ) ;
    ] ;
.
ct:SuffixType
  a rdfs:Datatype ;
  rdfs:comment "Denotes the suffix for a TIPLOC that identifies the instance on a circular route" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 1 ;
          ]
        ) ;
    ] ;
.
ct:TDAreaIDType
  a rdfs:Datatype ;
  rdfs:comment "A TD area identifier." ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 2 ;
          ]
        ) ;
    ] ;
.
ct:TDBerthIDType
  a rdfs:Datatype ;
  rdfs:comment "A TD berth identifier." ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 4 ;
          ]
        ) ;
    ] ;
.
ct:TOCType
  a rdfs:Datatype ;
  rdfs:comment "ATOC Code Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 2 ;
          ]
        ) ;
    ] ;
.
ct:TimetableFilenameType
  a rdfs:Datatype ;
  rdfs:comment "The name of a timetable file that can be downloaded via FTP." ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 1 ;
          ]
          [
            xsd:maxLength 128 ;
          ]
        ) ;
    ] ;
.
ct:TimetableIDType
  a rdfs:Datatype ;
  rdfs:comment "Unique Timetable identifier" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 14 ;
          ]
        ) ;
    ] ;
.
ct:TiplocType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:minLength 1 ;
          ]
          [
            xsd:maxLength 7 ;
          ]
        ) ;
    ] ;
.
ct:TrainIdType
  a rdfs:Datatype ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 4 ;
          ]
          [
            xsd:pattern "[0-9][A-Z][0-9][0-9]" ;
          ]
        ) ;
    ] ;
.
ct:TrainLengthType
  a rdfs:Datatype ;
  rdfs:comment "Defines the length of a train" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:unsignedShort ;
      owl:withRestrictions (
          [
            xsd:minInclusive "99"^^xsd:unsignedShort ;
          ]
        ) ;
    ] ;
.
ct:UIDType
  a rdfs:Datatype ;
  rdfs:comment "Train UID Type" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:length 6 ;
          ]
        ) ;
    ] ;
.
ct:WTimeType
  a rdfs:Datatype ;
  rdfs:comment "Working scheduled time as HH:MM[:SS]" ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
          [
            xsd:pattern "([0-1][0-9]|2[0-3]):[0-5][0-9](:[0-5][0-9])?" ;
          ]
        ) ;
    ] ;
.
ct:hasReasonCodeType
  a owl:DatatypeProperty ;
  rdfs:domain ct:DisruptionReasonType ;
  rdfs:range ct:ReasonCodeType ;
.
ct:near
  a owl:DatatypeProperty ;
  rdfs:comment "If true, the tiploc attribute should be interpreted as \"near\", e.g. \"signalling failure near Cheadle Hulme\"" ;
  rdfs:domain ct:DisruptionReasonType ;
  rdfs:range xsd:boolean ;
.
ct:pta
  a owl:DatatypeProperty ;
  rdfs:comment "Public time of arrival." ;
  rdfs:domain ct:CircularTimes ;
  rdfs:range ct:RTTITimeType ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct:ptd
  a owl:DatatypeProperty ;
  rdfs:comment "Public time of departure." ;
  rdfs:domain ct:CircularTimes ;
  rdfs:range ct:RTTITimeType ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct:tiploc
  a owl:DatatypeProperty ;
  rdfs:comment "Optional TIPLOC where the reason refers to, e.g. \"signalling failure at Cheadle Hulme\"." ;
  rdfs:domain ct:DisruptionReasonType ;
  rdfs:range ct:TiplocType ;
.
ct:wta
  a owl:DatatypeProperty ;
  rdfs:comment "Working time of arrival." ;
  rdfs:domain ct:CircularTimes ;
  rdfs:range ct:WTimeType ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct:wtd
  a owl:DatatypeProperty ;
  rdfs:comment "Working time of departure" ;
  rdfs:domain ct:CircularTimes ;
  rdfs:range ct:WTimeType ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct:wtp
  a owl:DatatypeProperty ;
  rdfs:comment "Working time of pass." ;
  rdfs:domain ct:CircularTimes ;
  rdfs:range ct:WTimeType ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
ct2:RSIDType
  a rdfs:Datatype ;
.
ct3:RSIDType
  a rdfs:Datatype ;
.
ct4:ToiletAvailabilityType
  a owl:Class ;
  rdfs:comment "The availability of a toilet in coach formation data. If no availability is supplied, it should be assumed to have the value \"Unknown\"." ;
.
ct4:ToiletStatus
  a rdfs:Datatype ;
  rdfs:comment "The service status of a toilet in coach formation data." ;
  owl:equivalentClass [
      a rdfs:Datatype ;
      owl:oneOf [
          a rdf:List ;
          rdf:first "InService" ;
          rdf:rest [
              a rdf:List ;
              rdf:first "NotInService" ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first "Unknown" ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
    ] ;
.
ct4:ToiletType
  a rdfs:Datatype ;
  rdfs:comment "An indication of the availability of a toilet in a coach in a train formation. E.g. \"Unknown\", \"None\" , \"Standard\" or \"Accessible\". Note that other values may be supplied in the future without a schema change" ;
  owl:equivalentClass xsd:string ;
.
ct4:status
  a owl:DatatypeProperty ;
  rdfs:comment "The service status of this toilet. E.g. \"Unknown\", \"InService\" or \"NotInService\"" ;
  rdfs:domain ct4:ToiletAvailabilityType ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:intersectionOf (
          ct4:ToiletStatus
          ct4:ToiletType
        ) ;
    ] ;
.
for:LateReason
  a owl:ObjectProperty ;
  rdfs:comment "Late running reason for this service. The reason applies to all locations of this service." ;
  rdfs:domain for:TS ;
  rdfs:range ct:DisruptionReasonType ;
.
for:Location
  a owl:ObjectProperty ;
  rdfs:comment "Update of forecast data for an individual location in the service's schedule" ;
  rdfs:domain for:TS ;
  rdfs:range for:TSLocation ;
.
for:PlatformData
  a owl:Class ;
  rdfs:comment "Platform number with associated flags" ;
.
for:TS
  a owl:Class ;
  rdfs:comment "Train Status. Update to the \"real time\" forecast data for a service." ;
.
for:TSLocation
  a owl:Class ;
  rdfs:comment "Forecast data for an individual location in the service's schedule" ;
.
for:TSTimeData
  a owl:Class ;
  rdfs:comment "Type describing time-based forecast attributes for a TS arrival/departure/pass" ;
.
for:arr
  a owl:ObjectProperty ;
  rdfs:comment "Forecast data for the arrival at this location" ;
  rdfs:domain for:TSLocation ;
  rdfs:range for:TSTimeData ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
for:at
  a owl:DatatypeProperty ;
  rdfs:comment "Actual Time" ;
  rdfs:domain for:TSTimeData ;
  rdfs:range ct:RTTITimeType ;
.
for:atClass
  a owl:DatatypeProperty ;
  rdfs:comment "The class of the actual time." ;
  rdfs:domain for:TSTimeData ;
  rdfs:range xsd:string ;
.
for:atRemoved
  a owl:DatatypeProperty ;
  rdfs:comment "If true, indicates that an actual time (\"at\") value has just been removed and replaced by an estimated time (\"et\"). Note that this attribute will only be set to \"true\" once, when the actual time is removed, and will not be set in any snapshot." ;
  rdfs:domain for:TSTimeData ;
  rdfs:range xsd:boolean ;
.
for:cisPlatsup
  a owl:DatatypeProperty ;
  rdfs:comment "Whether a CIS, or Darwin Workstation, has set platform suppression at this location." ;
  rdfs:domain for:PlatformData ;
  rdfs:range xsd:boolean ;
  rdfs:subPropertyOf owl:topDataProperty ;
.
for:conf
  a owl:DatatypeProperty ;
  rdfs:comment "True if the platform number is confirmed." ;
  rdfs:domain for:PlatformData ;
  rdfs:range xsd:boolean ;
.
for:delayed
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates that this estimated time is a forecast of \"unknown delay\". Displayed  as \"Delayed\" in LDB. Note that this value indicates that this forecast is \"unknown delay\", whereas it is the \"etUnknown\" attribute that indicates where the manual unknown delay forecast has been set." ;
  rdfs:domain for:TSTimeData ;
  rdfs:range xsd:boolean ;
.
for:dep
  a owl:ObjectProperty ;
  rdfs:comment "Forecast data for the departure at this location" ;
  rdfs:domain for:TSLocation ;
  rdfs:range for:TSTimeData ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
for:detachFront
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates from which end of the train stock will be detached. The value is set to “true” if stock will be detached from the front of the train at this location. It will be set at each location where stock will be detached from the front. Darwin will not validate that a stock detachment activity code applies at this location." ;
  rdfs:domain for:TSLocation ;
  rdfs:range xsd:boolean ;
.
for:et
  a owl:DatatypeProperty ;
  rdfs:comment "Estimated Time. For locations that are public stops, this will be based on the \"public schedule\". For operational stops and passing locations, it will be based on the \"working schedule\". It is only published where there is a corresponding \"activity\" for the service" ;
  rdfs:domain for:TSTimeData ;
  rdfs:range ct:RTTITimeType ;
.
for:etUnknown
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates that an unknown delay forecast has been set for the estimated time at this location. Note that this value indicates where a manual unknown delay forecast has been set, whereas it is the \"delayed\" attribute that indicates that the actual forecast is \"unknown delay\"." ;
  rdfs:domain for:TSTimeData ;
  rdfs:range xsd:boolean ;
.
for:etmin
  a owl:DatatypeProperty ;
  rdfs:comment "The manually applied lower limit that has been applied to the estimated time at this location. The estimated time will not be set lower than this value, but may be set higher." ;
  rdfs:domain for:TSTimeData ;
  rdfs:range ct:RTTITimeType ;
.
for:hasCircularTimes
  a owl:ObjectProperty ;
  rdfs:domain for:TSLocation ;
  rdfs:range ct:CircularTimes ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
for:hasPlatformData
  a owl:DatatypeProperty ;
  rdfs:domain for:PlatformData ;
  rdfs:range ct:PlatformType ;
.
for:hasPlatformType
  a owl:DatatypeProperty ;
  rdfs:range ct:PlatformType ;
  rdfs:subPropertyOf for:hasPlatformData ;
.
for:isReverseFormation
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates whether a train that divides is working with portions in reverse to their normal formation. The value applies to the whole train. Darwin will not validate that a divide association actually exists for this service." ;

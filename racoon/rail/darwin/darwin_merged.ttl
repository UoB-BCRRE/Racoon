sch3:Association
  a owl:Class ;
  rdfs:comment "Type describing an association between schedules" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch3:isCancelled ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch3:isDeleted ;
    ] ;
.
sch3:CallPtAttributes
  a owl:Class ;
  rdfs:comment "Group of attributes common to Calling Points" ;
.
sch3:DT
  a owl:Class ;
  rdfs:comment "Defines a Passenger Destination Calling point" ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ct:WTimeType ;
      owl:onProperty sch3:wtd ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sch3:CallPtAttributes ;
      owl:onProperty sch3:hasCallPtAttributes ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sch3:SchedLocAttributes ;
      owl:onProperty sch3:hasSchedLocAttributes ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange ct:WTimeType ;
      owl:onProperty sch3:wta ;
    ] ;
.

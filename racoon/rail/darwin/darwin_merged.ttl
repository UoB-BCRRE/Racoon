      owl:onProperty sch2:uid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct2:RSIDType ;
      owl:onProperty sch2:rsid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;

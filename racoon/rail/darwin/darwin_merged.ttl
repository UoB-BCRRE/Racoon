sch3:Schedule
  a owl:Class ;
  rdfs:comment "Train Schedule" ;
  rdfs:subClassOf [
      a owl:Class ;
      owl:intersectionOf (
          [
            a owl:Class ;
            owl:unionOf (
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:DT ;
                  owl:onProperty sch3:dt ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:IP ;
                  owl:onProperty sch3:ip ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:OPDT ;
                  owl:onProperty sch3:opdt ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:OPIP ;
                  owl:onProperty sch3:opip ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:OPOR ;
                  owl:onProperty sch3:opor ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:OR ;
                  owl:onProperty sch3:or ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch3:PP ;
                  owl:onProperty sch3:pp ;
                ]
              ) ;
          ]
          [
            a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass ct:DisruptionReasonType ;
            owl:onProperty sch3:cancelReason ;
          ]
        ) ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch3:deleted ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch3:isActive ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch3:isPassengerSvc ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:RIDType ;
      owl:onProperty sch3:rid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:RTTIDateType ;
      owl:onProperty sch3:ssd ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:TOCType ;
      owl:onProperty sch3:toc ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:TrainIdType ;
      owl:onProperty sch3:trainId ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:UIDType ;
      owl:onProperty sch3:uid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct3:RSIDType ;
      owl:onProperty sch3:rsid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.

sch2:Schedule
  a owl:Class ;
  rdfs:comment "Train Schedule" ;
  rdfs:subClassOf [
      a owl:Class ;
      owl:intersectionOf (
          [
            a owl:Class ;
            owl:unionOf (
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:DT ;
                  owl:onProperty sch2:dt ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:IP ;
                  owl:onProperty sch2:ip ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:OPDT ;
                  owl:onProperty sch2:opdt ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:OPIP ;
                  owl:onProperty sch2:opip ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:OPOR ;
                  owl:onProperty sch2:opor ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:OR ;
                  owl:onProperty sch2:or ;
                ]
                [
                  a owl:Restriction ;
                  owl:allValuesFrom sch2:PP ;
                  owl:onProperty sch2:pp ;
                ]
              ) ;
          ]
          [
            a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass ct:DisruptionReasonType ;
            owl:onProperty sch2:cancelReason ;
          ]
        ) ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ct:CIFTrainCategoryType ;
      owl:onProperty sch2:trainCat ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ct:CIFTrainStatusType ;
      owl:onProperty sch2:status ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch2:deleted ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch2:isActive ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty sch2:isPassengerSvc ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:RIDType ;
      owl:onProperty sch2:rid ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:RTTIDateType ;
      owl:onProperty sch2:ssd ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;

      owl:onProperty sch2:toc ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ct:TrainIdType ;
      owl:onProperty sch2:trainId ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;

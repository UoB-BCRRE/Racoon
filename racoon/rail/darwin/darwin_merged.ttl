tor:TrainOrder
  a owl:Class ;
  rdfs:comment "Defines the expected Train order at a platform" ;
.
tor:TrainOrderData
  a owl:Class ;
  rdfs:comment "Defines the sequence of trains making up the train order" ;
.
tor:TrainOrderItem
  a owl:Class ;
  rdfs:comment "Describes the identifier of a train in the train order" ;
.
tor:clear
  a owl:ObjectProperty ;
  rdfs:comment "Clear the current train order" ;
.
tor:clearTrainOrder
  a owl:Class ;
  rdfs:subClassOf tor:TrainOrder ;
.
tor:crs
  a owl:DatatypeProperty ;
  rdfs:comment "The CRS code of the station where the train order applies" ;
  rdfs:domain tor:TrainOrder ;
  rdfs:range ct:CrsType ;
.
tor:first
  a owl:ObjectProperty ;
  rdfs:comment "The first train in the train order." ;
  rdfs:domain tor:TrainOrderData ;
  rdfs:range tor:TrainOrderItem ;
.
tor:hasCircularTimes
  a owl:ObjectProperty ;
  rdfs:comment "One or more scheduled times to identify the instance of the location in the train schedule for which the train order is set." ;
  rdfs:domain tor:TrainOrderItem ;
  rdfs:range ct:CircularTimes ;
.
tor:platform
  a owl:DatatypeProperty ;
  rdfs:comment "The platform number where the train order applies" ;
  rdfs:domain tor:TrainOrder ;
  rdfs:range ct:PlatformType ;
.
tor:rid
  a owl:DatatypeProperty ;
  rdfs:comment "For trains in the train order where the train is the Darwin timetable, it will be identified by its RID" ;
  rdfs:domain tor:TrainOrderItem ;
  rdfs:range ct:RIDType ;
.
tor:ridTrainOrderItem
  a owl:Class ;
  rdfs:comment "For trains in the train order where the train is the Darwin timetable, it will be identified by its RID" ;
  rdfs:subClassOf tor:TrainOrderItem ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty tor:hasCircularTimes ;
      owl:someValuesFrom ct:CircularTimes ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty tor:rid ;
      owl:someValuesFrom ct:RIDType ;
    ] ;
.
tor:second
  a owl:ObjectProperty ;
  rdfs:comment "The second train in the train order." ;
  rdfs:domain tor:TrainOrderData ;
  rdfs:range tor:TrainOrderItem ;
.
tor:set
  a owl:ObjectProperty ;
  rdfs:domain tor:setTrainOrder ;
  rdfs:range tor:TrainOrderData ;
.
tor:setTrainOrder
  a owl:Class ;
  rdfs:subClassOf tor:TrainOrder ;
.
tor:third
  a owl:ObjectProperty ;
  rdfs:comment "The third train in the train order." ;
  rdfs:domain tor:TrainOrderData ;
  rdfs:range tor:TrainOrderItem ;
.
tor:tiploc
  a owl:DatatypeProperty ;
  rdfs:comment "The tiploc where the train order applies" ;
  rdfs:domain tor:TrainOrder ;
  rdfs:range ct:TiplocType ;
.
tor:trainID
  a owl:DatatypeProperty ;
  rdfs:comment "Where a train in the train order is not in the Darwin timetable, a Train ID (headcode) will be supplied" ;
  rdfs:domain tor:TrainOrderItem ;
  rdfs:range ct:TrainIdType ;
.
tor:trainIDTrainOrderItem
  a owl:Class ;
  rdfs:comment "Where a train in the train order is not in the Darwin timetable, a Train ID (headcode) will be supplied" ;
  rdfs:subClassOf tor:TrainOrderItem ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty tor:trainID ;
      owl:someValuesFrom ct:TrainIdType ;
    ] ;
.
